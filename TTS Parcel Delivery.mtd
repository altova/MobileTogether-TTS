	<XPathFunctionsRoot id="11">
		<children>
			<DesignXPathFunction id="165" xpath="declare function RemainingDestinations()
{
	let $packages := $USER/U/RemainingPackages/package/string()
	return $TODO/Todo/Driver/Destination[ @package = $packages ]
}"/>
			<DesignXPathFunction id="33917" xpath="declare function PartiallyFinishedDestinations()
{
	$USER/U/DeliverySummary/has-children() and count( RemainingDestinations() ) &gt; 0
}"/>
			<DesignXPathFunction id="183" xpath="declare function CurrentDestination()
{
	RemainingDestinations()[ xs:int( $USER/U/@NextDestination ) ]
}"/>
			<DesignXPathFunction id="33983" xpath="declare function NextDestination()
{
	RemainingDestinations()[ xs:int( $USER/U/@NextDestination ) + 1 ]
}"/>
			<DesignXPathFunction id="33943" xpath="declare function IsLastDestination()
{
	$USER/U/@NextDestination = count( RemainingDestinations() )
}"/>
			<DesignXPathFunction id="33421" xpath="declare function IsMetric()
{
	$USER/U/@Units = &apos;M&apos;
}"/>
			<DesignXPathFunction id="33343" xpath="declare function DistanceFromDestination()
{
	let $l := $MT_GEOLOCATION/Root/Location/@Geolocation return
	if ( $l ) then
		let $dkm := geolocation-distance-km( $l, CurrentDestination()/@geo )
		let $d := IF( IsMetric(), $dkm, $dkm * 0.62137 )
		let $threshold := IF( IsMetric(), 1, 0.5 ) return
	
		if ( $d &lt; $threshold ) then
			let $d := round( if ( IsMetric() ) then $d * 1000 else $d * 5280 )
			return map {
				&apos;d&apos; : $d,
				&apos;dkm&apos; : $dkm,
				&apos;df&apos; : mt-format-number( $d, &apos;0&apos; ),
				&apos;u&apos; : mt-load-string( IF( $USER/U/@Units = &apos;M&apos;, &apos;m&apos;, &apos;ft&apos; ) )
			}
		else
			let $d := round( $d, 1 )
			return map {
				&apos;d&apos; : $d,
				&apos;dkm&apos; : $dkm,
				&apos;df&apos; : mt-format-number( $d, &apos;0.0&apos; ),
				&apos;u&apos; : mt-load-string( IF( $USER/U/@Units = &apos;M&apos;, &apos;km&apos;, &apos;mi&apos; ) )
			}
	else
		()
}"/>
			<DesignXPathFunction id="34025" xpath="declare function Arrived()
{
	DistanceFromDestination() ?dkm &lt; 0.1
}"/>
			<DesignXPathFunction id="33548" xpath="declare function Parcels()
{
	let $p := CurrentDestination()/@package return
	$TODO/Todo/Delivery[ @package = $p ]/Parcel
}"/>
			<DesignXPathFunction id="33698" xpath="declare function Delivered()
{
	switch ( $USER/U/CurrentDelivery/@DeliveryTo )
	case &apos;C&apos; return true()
	case &apos;S&apos; return trim-string( $USER/U/CurrentDelivery/@NeighborName ) != &apos;&apos;
	default return false()
}"/>
			<DesignXPathFunction id="33570" xpath="declare function AnythingDamaged()
{
	Delivered() and $USER/U/CurrentDelivery/@AnythingDamaged = &apos;true&apos;
}"/>
			<DesignXPathFunction id="33731" xpath="declare function DamageResolutionText( $code as xs:string )
{
	mt-load-string( &apos;damage_&apos; || $code )
}"/>
			<DesignXPathFunction id="34198" xpath="declare function DamageReportResultsAcceptedParcels()
{
	switch ( $USER/U/CurrentDelivery/@AnythingDamaged )
	case &apos;true&apos; return some $DamagedParcel in  $USER/U/CurrentDelivery/DamagedParcels satisfies ($DamagedParcel/@resolution = &apos;&apos; or  $DamagedParcel/@resolution = &apos;Y&apos;)
	case &apos;false&apos; return true()
	default return true()
}"/>
			<DesignXPathFunction id="33622" xpath="declare function DamageReportConsistent()
{
	switch ( $USER/U/CurrentDelivery/@AnythingDamaged )
	case &apos;true&apos; return $USER/U/CurrentDelivery/DamagedParcels/@resolution != &apos;&apos;
	case &apos;false&apos; return true()
	default return false()
}"/>
			<DesignXPathFunction id="33307" xpath="declare function IF( $if, $true )
{
	if ( $if ) then $true else ()
}"/>
			<DesignXPathFunction id="33308" xpath="declare function IF( $if, $true, $else )
{
	if ( $if ) then $true else $else
}"/>
			<DesignXPathFunction id="33728" xpath="declare function NewLine( $n as xs:integer )
{
	repeat-string( &apos;
&apos;, $n )
}"/>
			<DesignXPathFunction id="34096" xpath="declare function IsDarkColor( $color )
{
	count( char( $color, (2,4,6) )[ . &gt;= &apos;0&apos; and . &lt;= &apos;7&apos; ] ) &gt;= 2
}"/>
			<DesignXPathFunction id="34241" xpath="declare function GetSigneeName()
{

	if ( $USER/U/CurrentDelivery/@DeliveryTo = &apos;C&apos; )
	then
		CurrentDestination()/@name
	else 
		$USER/U/CurrentDelivery/@NeighborName
}"/>
		</children>
	</XPathFunctionsRoot>
