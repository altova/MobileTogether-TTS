	<ActionGroupRoot id="160">
		<children>
			<DesignActionGroup id="241" name="Read Geolocation">
				<children>
					<DesignActionUpdateNode id="34095" needsClient="1" updAt="$USER/U/@Timestamp" resOf="high-res-timer()"/>
					<DesignActionGeolocationRead id="239" readAction="LatLngAddress"/>
					<DesignActionCondition id="33998" needsClient="1" condition="high-res-timer() - $USER/U/@Timestamp &gt; 2 and
$USER/U/@GeoUpdateFrequency != 0">
						<children>
							<DesignActionConditionTrue id="33999">
								<children>
									<DesignActionUpdateNode id="34105" needsClient="1" updAt="$USER/U/@GeoUpdateFrequency" resOf="0"/>
									<DesignActionMessageBox id="34106" msgBoxMsg="mt-load-string( &apos;geo slow&apos; )">
										<children>
											<DesignActionMessageBoxButton id="34107"/>
										</children>
									</DesignActionMessageBox>
								</children>
							</DesignActionConditionTrue>
							<DesignActionConditionFalse id="34007"/>
						</children>
					</DesignActionCondition>
				</children>
			</DesignActionGroup>
			<DesignActionGroup id="33358" name="Release Parcels">
				<children>
					<DesignActionGotoSubpage id="33370" pageRef="33362"/>
					<DesignActionCondition id="33591" needsClient="1" condition="$USER/U/CurrentDelivery/@Completed = &apos;true&apos;">
						<children>
							<DesignActionConditionTrue id="33592">
								<children>
									<DesignActionGroupRef id="33939" groupRef="33940"/>
								</children>
							</DesignActionConditionTrue>
						</children>
					</DesignActionCondition>
				</children>
			</DesignActionGroup>
			<DesignActionGroup id="33940" name="Proceed with Next Destination">
				<children>
					<DesignActionCondition id="33585" needsClient="1" condition="IsLastDestination()">
						<children>
							<DesignActionConditionTrue id="33586">
								<children>
									<DesignActionMessageBox id="33936" msgBoxMsg="mt-load-string( &apos;last delivery&apos; ) || NewLine( 2 ) || mt-load-string( &apos;view report&apos; )">
										<children>
											<DesignActionMessageBoxButton id="33937">
												<children>
													<DesignActionReturnFromSubpage id="33938"/>
												</children>
											</DesignActionMessageBoxButton>
										</children>
									</DesignActionMessageBox>
								</children>
							</DesignActionConditionTrue>
							<DesignActionConditionFalse id="33945">
								<children>
									<DesignActionUpdateNode id="33944" needsClient="1" updAt="$USER/U/@NextDestination" resOf="$USER/U/@NextDestination + 1"/>
									<DesignActionGroupRef id="34334" groupRef="34335"/>
								</children>
							</DesignActionConditionFalse>
						</children>
					</DesignActionCondition>
				</children>
			</DesignActionGroup>
			<DesignActionGroup id="34239" name="Complete Current Delivery">
				<children>
					<DesignActionUpdateNode id="34236" needsClient="1" updAt="$USER/U/CurrentDelivery/@Completed" resOf="true()"/>
					<DesignActionAppendNode id="34237" needsClient="1" appAt="$USER/U/DeliverySummary" resOf="$USER/U/CurrentDelivery !
(
	let $any-damaged := Delivered() and @AnythingDamaged = &apos;true&apos;

	let $damaged-quota:= 
			if ( $any-damaged ) then
				if			( every  $i in DamagedParcels satisfies $i/@resolution = &apos;N&apos; ) then &apos;R&apos;
				else if	( some $i in DamagedParcels satisfies $i/@resolution = &apos;N&apos; ) then &apos;P&apos;
				else ()
			else ()

	return element Entry
	{
			CurrentDestination() ! ( @package, @name, @company ),
			attribute signature { @Signature },
			attribute action { format-string( mt-load-string( &apos;delivered_&apos; || @DeliveryTo || $damaged-quota ), @NeighborName ) },
	
			if ( $any-damaged )
			then DamagedParcels[ @resolution != &apos;&apos; ]
			else ()
		}
)"/>
				</children>
			</DesignActionGroup>
			<DesignActionGroup id="34335" name="Vocalize Next Destination">
				<children>
					<DesignActionCondition id="34421" needsClient="1" condition="$USER/U/@SpeakDuringDelivery = &apos;true&apos;">
						<children>
							<DesignActionConditionTrue id="34422">
								<children>
									<DesignActionTextToSpeech id="34420" needsClient="1" speechText="concat(&apos;Your next destination is &apos; ,
CurrentDestination() ! (
@company || &apos;
&apos; || @address ),
&apos;    You are &apos;,
DistanceFromDestination() ! format-string( mt-load-string( &apos;away&apos; ), ( ?df, ?u ) )
 )"/>
								</children>
							</DesignActionConditionTrue>
						</children>
					</DesignActionCondition>
				</children>
			</DesignActionGroup>
			<DesignActionGroup id="34341" name="Vocalize New Distance to Destination">
				<children>
					<DesignActionLoop id="34409" needsClient="1" needsClientView="1" breakLoopIf="mt-text-to-speech-is-speaking() = false()">
						<children>
							<DesignActionUpdateNode id="34410" updAt="$PERSISTENT/Root" resOf="sleep(100)"/>
						</children>
					</DesignActionLoop>
					<DesignActionCondition id="34417" needsClient="1" condition="( $USER/U/@SpeakDuringDelivery = &apos;true&apos; )">
						<children>
							<DesignActionConditionTrue id="34418">
								<children>
									<DesignActionTextToSpeech id="34419" needsClient="1" speechText="concat( 
&apos;You are &apos;,
DistanceFromDestination() ! format-string( mt-load-string( &apos;away&apos; ), ( ?df, ?u ) )
)"/>
								</children>
							</DesignActionConditionTrue>
						</children>
					</DesignActionCondition>
				</children>
			</DesignActionGroup>
		</children>
